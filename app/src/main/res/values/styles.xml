<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:statusBarColor">@color/brown</item>
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/primaryColor</item>
        <item name="colorPrimaryDark">@color/primaryDarkColor</item>
        <item name="colorAccent">@color/secondaryColor</item>
    </style>

    <style name="Theme.App" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/brown</item>
        <item name="colorSecondary">@color/whitish</item>
        <item name="android:colorFocusedHighlight">@color/tw__blue_default</item>
    </style>
</resources>
